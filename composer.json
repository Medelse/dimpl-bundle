{
    "name": "medelse/dimpl-bundle",
    "type": "symfony-bundle",
    "description": "The DimplBundle is a Symfony Bundle to request Dimpl API",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^5.4",
        "symfony/dotenv": "^5.4",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/mime": "^5.4",
        "symfony/options-resolver": "^5.4",
        "symfony/runtime": "^5.4",
        "symfony/yaml": "^5.4",
        "symfony/cache": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^5.4",
        "symfony/css-selector": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "ext-json": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Medelse\\DimplBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Medelse\\DimplBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
